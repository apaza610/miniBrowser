<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.web.WebView?>

<AnchorPane prefHeight="200.0" prefWidth="200.0" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.apaza.miapp.VisualizadorCtrl">
    <VBox>
        <HBox fillHeight="false" VBox.vgrow="ALWAYS">
            <Button mnemonicParsing="false" onAction="#goToSite" text="go" HBox.hgrow="NEVER" />
            <Button mnemonicParsing="false" onAction="#onFit" text="fit" HBox.hgrow="NEVER" />
            <TextField fx:id="tfInput" promptText="...path" HBox.hgrow="ALWAYS" />
        </HBox>
        <WebView minHeight="-1.0" minWidth="-1.0" prefHeight="200.0" prefWidth="200.0" />
    </VBox>
</AnchorPane>
